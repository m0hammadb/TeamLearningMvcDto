@model List<Account>

<div class="container">
    <div class="row">

        <div class="col-md-8 mx-auto">
            <table class="table table-bordered table-striped">
                <tr>
                    <th>
                        Name
                    </th>

                    <th>
                        Email
                    </th>
                </tr>

                @foreach(Account account in Model)
                {
                    <tr>
                        <td>
                            @account.Name
                        </td>

                        <td>
                            @account.Email
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>